{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.github": true,
    "**/.gitp*": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.vscode": true,
    "**/core.Microsoft*": true,
    "**/core.mongo*": true,
    "**/core.python*": true,
    "**/CVS": true
  },
  "files.autoSave": "off",
  "workbench.colorTheme": "Tokyo Night Storm",
  "terminal.integrated.gpuAcceleration": "canvas",
  "cSpell.words": [
    "Allauth",
    "blanksearch",
    "csrfmiddlewaretoken",
    "cust",
    "expandright",
    "fretboard",
    "invalidemail",
    "johndoe",
    "lineitem",
    "lineitems",
    "postloadjs",
    "searchbar",
    "searchbutton",
    "Stratocaster",
    "testpass"
  ]
}
